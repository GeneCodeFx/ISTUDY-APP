{
  "expo": {
    "name": "ISTUDY",
    "slug": "ISTUDY",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.istudycompany.ISTUDY"
    },
    "android": {
      "package": "com.istudycompany.ISTUDY",
      "buildNumber": "1.0.0",
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "description": "check if its responsive to all screens ios and android",
    "githubUrl": "https://github.com/khetzo/ISTUDY-APP.git"
  }
}
